{{#each err_msg}}
    <div class="alert alert-warning alert-dismissible fade show" role="alert">
        {{this.msg}}
        <button type="button" class="close" data-dismiss="alert" aria-label="clsoe">
            <span aria-hiden="true">&times;</span>
        </button>

    </div>
{{/each}}

{{#if success_msg}}
<div class="alert alert-success alert-dismissible fade show" role="alert">
        {{this.success_msg}}
        <button type="button" class="close" data-dismiss="alert" aria-label="clsoe">
            <span aria-hiden="true">&times;</span>
        </button>

    </div>

{{/if}}



{{#if error}}
<div class="alert alert-success alert-dismissible fade show" role="alert">
        {{this.error}}
        <button type="button" class="close" data-dismiss="alert" aria-label="clsoe">
            <span aria-hiden="true">&times;</span>
        </button>

    </div>

{{/if}}



